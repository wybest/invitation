<script>
 $(function(){
    var str= location.href; 
    str=str.substring(str.indexOf("=")+1,str.indexOf("&")!=-1?str.indexOf("&"):100)


    //去除全部空格
    function trim(str){
        str = str.replace(/\s+/g,"");
        return str;
    };
    
    $(".btnMsg").click(function(){


        var guest = $('#guest').val();
        var zf = $('#message').val();
        var pnum = $('#pnum').val();
        if(guest == ''){
            alert('请填写姓名');
            return;
        }
        if(zf == ''){
            alert('请填写留言');
            return;
        }

        var username='{#$name#}';

        $.post('save_shuju.php', {user:username, guest:encodeURIComponent(guest), zf:encodeURIComponent(zf), pnum:encodeURIComponent(pnum)}, function (data){
            $('#bi_msg').html(unescape(data)).show();
            setTimeout(function (){
                $('#bi_msg').fadeOut();
            }, 10000);
        });

    })



